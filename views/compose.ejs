<%- include("partials/header") %>


<h1>Compose</h1>


<form class="form-group" action="/compose" method="post">
  <div class="form-group">
    <label for="Inputtext"><h3>Title</h3></label>
    <input type="text" class="form-control" id="Inputtext" placeholder="Title of Blog" name="postTitle">
  </div>
  <div class="form-group">
    <label for="textToInput"><h3>Password</h3></label>
    <textarea  type="text" class="form-control" placeholder="New Blog" name="postBody" rows="8" cols="80" id="textToInput"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Add New Blog</button>
</form>


<%- include("partials/footer") %>
